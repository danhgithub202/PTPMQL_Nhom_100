<!--The Submit Button-->
<form action="/action_page.php">
  <label for="fname">Đào Sông Danh - 2121050308</label><br>
  <input type="text" id="fname" name="fname" value="Đào Sông Danh - 2121050308"><br>
  <label for="lname">Đào Sông Danh - 2121050308</label><br>
  <input type="text" id="lname" name="lname" value="Đào Sông Danh - 2121050308"><br><br>
  <input type="submit" value="Đào Sông Danh - 2121050308">
</form>

<!--The Name Attribute Example-->
